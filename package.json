{
  "name": "peak-blooms",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "biome lint --write .",
    "format": "biome format --write .",
    "postinstall": "prisma generate",
    "knip": "dotenv -e .env.local -- knip",
    "db:docker:up": "docker-compose up -d postgres && sleep 3 && npm run db:docker:push && npm run db:docker:reset && npm run db:docker:seed",
    "db:docker:down": "docker-compose down",
    "db:docker:generate": "dotenv -e .env.local -- prisma generate",
    "db:docker:push": "dotenv -e .env.local -- prisma db push",
    "db:docker:dev": "dotenv -e .env.local -- prisma migrate dev",
    "db:docker:reset": "dotenv -e .env.local -- prisma migrate reset --force && npm run db:docker:seed",
    "db:docker:seed": "dotenv -e .env.local -- prisma db seed",
    "db:prod:reset": "dotenv -e .env.prod -- prisma migrate reset --force && db:prod:seed",
    "db:prod:seed": "dotenv -e .env.prod -- prisma db seed",
    "db:docker:studio": "dotenv -e .env.local -- prisma studio",
    "db:prod:studio": "dotenv -e .env.prod -- prisma studio"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "7.1.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@vercel/blob": "^2.0.0",
    "better-auth": "^1.4.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^17.2.3",
    "libphonenumber-js": "^1.12.31",
    "lucide-react": "^0.559.0",
    "next": "^16.0.8",
    "pg": "^8.16.3",
    "prisma": "7.1.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-hook-form": "^7.68.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24.10.2",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv-cli": "^11.0.0",
    "knip": "^5.73.1",
    "tailwindcss": "^4.1.17",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  }
}
