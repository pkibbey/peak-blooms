{
  "name": "peak-blooms",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "biome lint --write .",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "format": "biome format --write .",
    "postinstall": "node ./scripts/postinstall.js",
    "knip": "dotenv -e .env.local -- knip",
    "db:docker:up": "docker-compose up -d postgres && sleep 3 && npm run db:docker:push && npm run db:docker:reset && npm run db:docker:seed",
    "db:docker:down": "docker-compose down",
    "db:docker:generate": "dotenv -e .env.local -- prisma generate",
    "db:docker:push": "dotenv -e .env.local -- prisma db push",
    "db:docker:dev": "dotenv -e .env.local -- prisma migrate dev",
    "db:docker:reset": "dotenv -e .env.local -- prisma migrate reset --force",
    "db:docker:seed": "dotenv -e .env.local -- prisma db seed",
    "db:prod:generate": "prisma generate",
    "db:prod:reset": "dotenv -e .env.prod -- prisma migrate reset --force",
    "db:prod:seed": "dotenv -e .env.prod -- prisma db seed",
    "db:docker:studio": "dotenv -e .env.local -- prisma studio",
    "db:prod:studio": "dotenv -e .env.prod -- prisma studio",
    "prepare": "husky"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "7.1.0",
    "@vercel/blob": "^2.0.0",
    "add": "^2.0.6",
    "better-auth": "1.4.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "libphonenumber-js": "^1.12.31",
    "lucide-react": "^0.561.0",
    "next": "^16.0.10",
    "pg": "^8.16.3",
    "prisma": "7.1.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.68.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^24.10.4",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv-cli": "^11.0.0",
    "husky": "^9.1.7",
    "knip": "^5.75.1",
    "lint-staged": "^16.2.7",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json}": [
      "biome format --write",
      "biome lint --write"
    ]
  }
}
